<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- Tall grass -->
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[defName = "SeedGlowstool"]</xpath>
				<value>
					<extractable>False</extractable>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[defName = "SeedAgarilux"]</xpath>
				<value>
					<extractable>False</extractable>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="RWC_SeedBase"]/graphicData</xpath>
				<value>
					<graphicData>
						<texPath>Seeds/Seeds</texPath>
						<graphicClass>Graphic_Single</graphicClass>
					</graphicData>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>
